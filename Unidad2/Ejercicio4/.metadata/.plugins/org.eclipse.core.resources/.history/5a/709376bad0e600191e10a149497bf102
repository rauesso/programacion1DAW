import java.util.Scanner;

public class Practica6 {
	public static void main(String[] args) {
	int num1, num2;
	int intercambio;
	Scanner lector=new Scanner(System.in);
	System.out.println("Dame un numero y pulsa Intro:");
	num1=lector.nextInt();
	System.out.println("Dame otro numero y pulsa Intro:");
	num2=lector.nextInt();
	System.out.println("Los valores proporcionados son num1: "+num1+"y num 2: "+num2);
	intercambio=num1;
	num1=num2;
	num2=intercambio;
	System.out.println("Los valores intercambiados son num1: "+num1+"y num 2: "+num2);
	}

}
